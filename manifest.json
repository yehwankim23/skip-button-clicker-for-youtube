{
  "manifest_version": 3,
  "name": "Skip Button Clicker for YouTube™",
  "version": "2023.11",

  "description": "Chrome extension that clicks skip buttons on skippable advertisements on YouTube™",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "author": "김예환 Ye-Hwan Kim (Sam)",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["script.js"]
    }
  ],
  "homepage_url": "https://github.com/yehwankim23/skip-button-clicker-for-youtube"
}
